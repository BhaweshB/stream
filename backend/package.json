{
  "name": "rtsp-streaming-backend",
  "version": "1.0.0",
  "description": "Professional RTSP to HLS streaming backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "docker:build": "docker build -t rtsp-streamer .",
    "docker:run": "docker run -p 3000:3000 -p 8888:8888 rtsp-streamer"
  },
  "keywords": ["rtsp", "hls", "streaming", "ffmpeg"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ws": "^8.14.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "wrtc": "^0.4.7",
    "node-rtsp-stream": "^0.0.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/node": "^20.10.5",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
